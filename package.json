        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "express": "^4.17.1",
        "file-loader": "^5.1.0",
        "html-webpack-plugin": "^3.2.0",
        "identity-obj-proxy": "^3.0.0",
        "image-webpack-loader": "^6.0.0",
        "jest": "^25.1.0",
        "less": "^3.11.1",
