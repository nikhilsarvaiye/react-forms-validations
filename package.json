{
    "name": "react-forms",
    "version": "0.1.0",
    "description": "React forms bootstrap repository around formik and you with themes",
    "author": "Nikhil Sarvaiye",
    "license": "UNLICENSED",
    "private": true,
    "homepage": "https://github.com/nikhilsarvaiye/react-forms",
    "repository": {
        "type": "git",
        "url": "https://github.com/nikhilsarvaiye/react-forms.git"
    },
    "scripts": {
        "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod",
        "clean-dist": "rimraf dist/*",
        "prettier-write": "prettier --write \"{src,tests}/**/*.{js,jsx,css,scss}\"",
        "prettier-check": "prettier --list-different \"{src,tests}/**/*.{js,jsx,css,scss}\"",
        "start": "npm run start-dev",
        "start-dev": "webpack-dev-server --config=configs/webpack/dev",
        "start-prod": "npm run build && node express.js",
        "test": "jest --watchAll --coverage --config=configs/jest.json"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
