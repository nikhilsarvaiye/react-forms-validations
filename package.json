        "@hot-loader/react-dom": "^16.12.0",
        "babel-jest": "^26.0.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-import": "^1.13.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-plugin-syntax-async-functions": "^6.13.0",
